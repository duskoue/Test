
    <ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:ApiDocument">

    <ImageSource x:Key="Save">icon/Save.ico</ImageSource>
    <ImageSource x:Key="Folder">icon/Folder.png</ImageSource>
    <ImageSource x:Key="Help">icon/Help symbol.ico</ImageSource>
    <ImageSource x:Key="Cut">icon/Cut.ico</ImageSource>
    <ImageSource x:Key="Copy">icon/Copy.ico</ImageSource>
    <ImageSource x:Key="Paste">icon/Paste.ico</ImageSource>
    <ImageSource x:Key="duplicate">icon/dp.png</ImageSource>


    <Style x:Key="fontstil" TargetType="{x:Type Control}">
        <Setter Property="Foreground" Value="Black" />
        <Setter Property="FontSize" Value="18" />
        <Setter Property="FontFamily" Value="Courier New" />
        <Setter Property="FontStyle" Value="Normal" />
    </Style>
    <local:CApiDoc x:Key="Apires"/>
    <local:CUIParam x:Key="Ulparam"/>
    <Style TargetType="DataGrid">
        <Setter Property="TextBlock.TextWrapping" Value="Wrap" />
    </Style>
</ResourceDictionary>