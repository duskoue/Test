<Window x:Class="ApiDocument.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ApiDocument"
        xmlns:RW="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
        mc:Ignorable="d"
        Title="Api dokumentacija izveštaj" Height="918.082" Width="1040.017" Loaded="Window_Loaded" Closing="Window_Closing">
    <Window.CommandBindings>
        <CommandBinding x:Name="KreirajPDF" Command="{x:Static local:CKomandeApi.KreirajPDF}" CanExecute="KreirajPDF_CanExecute" Executed="KreirajPDF_Executed"/>
    </Window.CommandBindings>
    <StackPanel Margin="0,0,0,5">
        <Button x:Name="btnPDF" Content="Published PDF" HorizontalAlignment="Center" Margin="466,10,466,0" VerticalAlignment="Top" Width="100" Click="btnPDF_Click" RenderTransformOrigin="0.467,-0.1"/>
        <WindowsFormsHost Height="853" Margin="0,2,2,0" VerticalAlignment="Bottom">
            <RW:ReportViewer x:Name="Izvestaj" BorderStyle="Fixed3D" />

        </WindowsFormsHost>



    </StackPanel>



</Window>
